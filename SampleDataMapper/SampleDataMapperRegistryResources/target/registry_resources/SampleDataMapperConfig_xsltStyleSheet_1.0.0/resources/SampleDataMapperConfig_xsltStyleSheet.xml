<?xml version="1.0" encoding="UTF-8" standalone="no"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:firstElementOfTheInput="Employee" xmlns:notXSLTCompatible="false" xmlns:own="http://ownfunction.org" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema" extension-element-prefixes="xs own notXSLTCompatible firstElementOfTheInput" version="2.0"><xsl:function name="own:setPrecision"><xsl:param name="resultString"/><xsl:param name="noOfDigits"/><xsl:if test="$noOfDigits=0"><xsl:value-of select="$resultString"/></xsl:if><xsl:if test="$noOfDigits!=0"><xsl:value-of select="own:setPrecision(concat($resultString,'0'),$noOfDigits - 1)"/></xsl:if></xsl:function><xsl:template match="/"><Employee><ID><xsl:value-of select="Employee/ID"/></ID><Name><FullName><xsl:value-of select="concat(Employee/FirstName,' ',Employee/LastName)"/></FullName><FirstName><xsl:value-of select="Employee/FirstName"/></FirstName><LastName><xsl:value-of select="Employee/LastName"/></LastName></Name><Address><Address1><xsl:value-of select="tokenize(Employee/Address,',')[1]"/></Address1><Address2><xsl:value-of select="tokenize(Employee/Address,',')[2]"/></Address2></Address></Employee></xsl:template></xsl:stylesheet>